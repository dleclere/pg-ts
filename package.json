{
  "name": "pg-ts",
  "version": "2.4.1",
  "main": "dist/index.js",
  "types": "dist/",
  "module": "src/index.ts",
  "repository": "https://github.com/musicglue/pg-ts",
  "author": "Music Glue",
  "license": "MIT",
  "scripts": {
    "build": "rm -rf dist/  && tsc",
    "format": "prettier --write --list-different --parser ${npm_package_prettier_parser} --trailing-comma ${npm_package_prettier_trailingComma} --print-width ${npm_package_prettier_printWidth} --arrow-parens ${npm_package_prettier_arrow_parens} 'src/**/*.ts'",
    "p3epublishOnly": "npm run build",
    "test": "NODE_ENV=test jest --runInBand"
  },
  "jest": {
    "collectCoverage": true,
    "coverageDirectory": "./coverage",
    "coveragePathIgnorePatterns": [
      "<rootDir>/knexfile.ts",
      "<rootDir>/src/db.ts",
      "<rootDir>/build/",
      "<rootDir>/node_modules/",
      "<rootDir>/__tests__/support/*.ts"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 75,
        "functions": 75,
        "lines": 75,
        "statements": 75
      }
    },
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/__tests__/setup.ts",
    "transform": {
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "/__tests__/.*_test\\.(ts|tsx|js)$"
  },
  "dependencies": {
    "fp-ts": "0.6.8",
    "lodash": "^4.17.4",
    "pg": "^8.2.1",
    "postgres-array": "^2.0.0"
  },
  "devDependencies": {
    "@types/jest": "^22.1.1",
    "@types/lodash": "^4.14.99",
    "@types/pg": "^7.4.1",
    "jest": "^22.1.4",
    "prettier": "^2.0.5",
    "ts-jest": "^22.0.2",
    "ts-node": "^4.1.0",
    "tslint": "^5.9.1",
    "typescript": "^2.7.1"
  },
  "prettier": {
    "arrowParens": "avoid",
    "parser": "typescript",
    "printWidth": 100,
    "trailingComma": "all"
  }
}
